{
    "name": "xpdeal/omnipay-eupago",
    "description": "EuPago driver for Omnipay payment processing library",
    "type": "library",
    "require": {
        "omnipay/common": "^3.2",
        "symfony/http-client": "^6.3",
        "php-http/httplug": "^2.4",
        "nyholm/psr7": "^1.8",
        "psr/http-factory": "^1.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "nunomaduro/phpinsights": "^2.11"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Omnipay\\EuPago\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jordan Carvalho",
            "email": "jordan@xpdeal.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
